<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:properties="clr-namespace:WpfApplication1.Properties"
                    xmlns:img="clr-namespace:WpfApplication1">
    <BitmapImage x:Key="Renameico" UriSource="../rename.ico"/>
    
    <Style x:Key="ButtonFocusVisual">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border>
                        <Rectangle SnapsToDevicePixels="true" Margin="4" Stroke="Black" StrokeDashArray="1 2" StrokeThickness="1"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0" x:Key="TopBlack">
        <GradientStop Color="Black" Offset="0.1"/>
        <GradientStop Color="Transparent" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0" x:Key="BotBlack">
        <GradientStop Color="Transparent" Offset="0.1"/>
        <GradientStop Color="Black"  Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0" x:Key="TopGreen">
        <GradientStop Color="Green" Offset="0.3"/>
        <GradientStop Color="Transparent" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0" x:Key="BotGreen">
        <GradientStop Color="Transparent" Offset="0"/>
        <GradientStop Color="Green"  Offset="0.7"/>
    </LinearGradientBrush>

    <LinearGradientBrush EndPoint="1,1" StartPoint="0,0" x:Key="LeftGreen">
        <GradientStop Color="Green" Offset="0"/>
        <GradientStop Color="Transparent" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush EndPoint="1,0" StartPoint="0,0" x:Key="RightGreen">
        <GradientStop Color="Transparent" Offset="0"/>
        <GradientStop Color="Green"  Offset="1"/>
    </LinearGradientBrush>


    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0" x:Key="TopRed">
        <GradientStop Color="Red" Offset="0.3"/>
        <GradientStop Color="Transparent" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0" x:Key="BotRed">
        <GradientStop Color="Transparent" Offset="0.3"/>
        <GradientStop Color="Red"  Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0" x:Key="TopBlue">
        <GradientStop Color="Blue" Offset="0.3"/>
        <GradientStop Color="Transparent" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0" x:Key="BotBlue">
        <GradientStop Color="Transparent" Offset="0.3"/>
        <GradientStop Color="Blue"  Offset="1"/>
    </LinearGradientBrush>

    <GradientStopCollection x:Key="My2ndGlassRadialGradientStopsResource">
        <GradientStop Color="Green" Offset="0.2" />
        <GradientStop Color="CadetBlue" Offset="0.4" />
        <GradientStop Color="Transparent" Offset="0.5" />
        <GradientStop Color="CadetBlue" Offset="0.75" />
        <GradientStop Color="AliceBlue" Offset="0.9" />
        <GradientStop Color="Transparent" Offset="1" />
    </GradientStopCollection>

    <RadialGradientBrush x:Key="My2ndGlassRadialBrushResource" GradientOrigin="0.5,0.5" Center="0.5,0" 
          RadiusX="0.5" RadiusY="0.5"
        GradientStops="{StaticResource My2ndGlassRadialGradientStopsResource}" />

    <GradientStopCollection x:Key="My3rdGlassRadialGradientStopsResource">
        <GradientStop Color="Transparent" Offset="0.2" />
        <GradientStop Color="CadetBlue" Offset="0.4" />
        <GradientStop Color="Transparent" Offset="0.5" />
        <GradientStop Color="CadetBlue" Offset="0.75" />
        <GradientStop Color="White" Offset="0.9" />
        <GradientStop Color="Transparent" Offset="1" />
    </GradientStopCollection>

    <RadialGradientBrush x:Key="My3rdGlassRadialBrushResource" GradientOrigin="0.5,0.5" Center="0.5,1" 
          RadiusX="0.5" RadiusY="0.5"
        GradientStops="{StaticResource My3rdGlassRadialGradientStopsResource}" />


    <GradientStopCollection x:Key="My5GlassRadialGradientStopsResource">
